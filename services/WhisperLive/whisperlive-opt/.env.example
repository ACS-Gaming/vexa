# WhisperLive Optimization Environment Variables

# LLM API Keys (for quality assessment)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# WhisperLive Server Configuration
WHISPER_SERVER_URL=ws://whisperlive-server:9090/ws
WHISPER_SERVER_HOST=whisperlive-server
WHISPER_SERVER_PORT=9090

# Optimization Settings
DEFAULT_CONCURRENCY=32
DEFAULT_RUN_DURATION=120
DEFAULT_WARMUP_DURATION=20

# GPU Monitoring
ENABLE_GPU_MONITORING=true
GPU_SAMPLE_INTERVAL=1.0

# Logging
LOG_LEVEL=INFO
ENABLE_DASHBOARD=true

# Data Paths
DATA_DIR=/app/data
RESULTS_DIR=/app/results
CACHE_DIR=/app/cache
